{
  "name": "nest-starter-kit",
  "version": "0.0.1",
  "description": "Nest Starter Kit",
  "author": "Nguyen Quang <ngocquang994@gmail.com>",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts,.js \"src/**\"",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/ormconfig_migration.ts",
    "typeorm:migration:create": "yarn typeorm migration:create -d src/migrations",
    "migration:run": "yarn typeorm migration:run -t=false",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.1",
    "@nestjs/bull": "0.1.1",
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/graphql": "7.3.7",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/microservices": "7.0.9",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "2.1.0",
    "@nestjs/typeorm": "7.0.0",
    "accesscontrol": "2.2.1",
    "apollo-server": "2.13.0",
    "apollo-server-express": "2.13.0",
    "apollo-server-plugin-base": "0.8.0",
    "apple-auth": "^1.0.7",
    "apple-sign-in-rest": "^1.0.3",
    "apple-signin": "^1.0.9",
    "apple-signin-auth": "^1.5.1",
    "aws-sdk": "2.669.0",
    "axios": "0.19.2",
    "bcryptjs": "^2.4.3",
    "bull": "3.13.0",
    "cache-manager": "3.3.0",
    "casbin": "4.7.0",
    "child_process": "^1.0.2",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cookie-session": "1.4.0",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "exceljs": "^3.9.0",
    "fb": "^2.0.0",
    "firebase-admin": "^10.0.0",
    "fs": "^0.0.1-security",
    "google-libphonenumber": "^3.2.25",
    "graphql": "^15.6.1",
    "graphql-query-complexity": "^0.9.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-tools": "5.0.0",
    "graphql-voyager": "1.0.0-rc.30",
    "handlebars": "4.7.6",
    "helmet": "3.22.0",
    "inline-css": "2.6.3",
    "jwt-decode": "2.2.0",
    "moment": "2.25.3",
    "nanoid": "3.1.4",
    "nestjs-multer-extended": "^1.4.2",
    "nestjs-typeorm-paginate": "2.0.3",
    "node-fetch": "^2.6.0",
    "node-pre-gyp": "^0.17.0",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.0",
